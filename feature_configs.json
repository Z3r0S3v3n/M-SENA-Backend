{
  "audio": {
    "librosa": {
      "audio": {
        "tool": "librosa",
        "sample_rate": 16000,
        "args": {
          "mfcc": {
            "n_mfcc": 20,
            "htk": true
          },
          "rms": {},
          "zero_crossing_rate": {},
          "spectral_rolloff": {},
          "spectral_centroid": {}
        }
      }
    },
    "opensmile": {
      "audio": {
        "tool": "opensmile",
        "sample_rate": 16000,
        "args": {
          "feature_set": "eGeMAPS",
          "feature_level": "Functionals",
          "start": null,
          "end": null
        }
      }
    },
    "wav2vec": {
      "audio": {
        "tool": "wav2vec",
        "sample_rate": 16000,
        "pretrained": "facebook/wav2vec2-base-960h"
      }
    }
  },
  "visual": {
    "mediapipe": {
      "video": {
        "tool": "mediapipe",
        "fps": 25,
        "args": {
          "face_mesh": {
            "refine_landmarks": true,
            "min_detection_confidence": 0.35,
            "min_tracking_confidence": 0.5
          },
          "visualize": false,
          "visualize_dir": "~/.MMSA-FET/visualize"
        }
      }
    },
    "openface2": {
      "video": {
        "tool": "openface2",
        "fps": 25,
        "args": {}
      }
    },
    "vggface": {
      "video": {
        "tool": "vggface",
        "fps": 25,
        "args": {}
      }
    }
  },
  "text": {
    "bert": {
      "text": {
        "model": "bert",
        "device": "cpu",
        "pretrained": "bert-base-uncased",
        "finetune": "false"
      }
    },
    "xlnet": {
      "text": {
        "model": "xlnet",
        "device": "cpu",
        "pretrained": "xlnet-base-cased",
        "finetune": "false"
      }
    }
  }
}
